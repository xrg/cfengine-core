  "inputs/Makefile" ) CONFIG_FILES="$CONFIG_FILES inputs/Makefile" ;;
  "Makefile" ) CONFIG_FILES="$CONFIG_FILES Makefile" ;;
  "doc/Makefile" ) CONFIG_FILES="$CONFIG_FILES doc/Makefile" ;;
  "src/cflex.l" ) CONFIG_FILES="$CONFIG_FILES src/cflex.l" ;;
  "contrib/vicf" ) CONFIG_FILES="$CONFIG_FILES contrib/vicf" ;;
  "default-1" ) CONFIG_COMMANDS="$CONFIG_COMMANDS default-1" ;;
  "src/conf.h" ) CONFIG_HEADERS="$CONFIG_HEADERS src/conf.h" ;;
  *) { { echo "$as_me:7861: error: invalid argument: $ac_config_target" >&5
echo "$as_me: error: invalid argument: $ac_config_target" >&2;}
   { (exit 1); exit 1; }; };;
  esac
done

